@using System
@using System.Collections.Generic
@using Web.Models

<div class="container py-4 py-xl-5">
    <div class="row mb-5">
        <div class="col-md-8 col-xl-6 text
-center mx-auto">
            <h2>Reportes</h2>
            <p></p>
        </div>
    </div>
    <form asp-action="InicioReportes" asp-controller="Reportes" method="post">
        <div class="row gy-4 row-cols-1 row-cols-md-2 row-cols-xl-3">
            <div class="col">
                <div class="card">
                    <div class="card-body p-4">
                        <p class="text-primary card-text mb-0">Article</p>
                        <h4 class="card-title">Reportes de cantidad por fechas </h4>
                        <p class="card-text"></p>
                        <div class="d-flex">
                            <a href="/Reportes/GenerarPDF" class="btn btn-primary" role="button">Generar PDF</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>




